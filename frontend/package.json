{
  "name": "bookings-manager-frontend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "start \"\" npm run web:start & start \"\" npm run script:start & start \"\" npm run build-ts:watch",
    "start:windows": "start /b npm run web:start & start /b npm run script:start & start /b npm run build-ts:watch",
    "start:unix": "npm run web:start & npm run script:start & npm run build-ts:watch",
    "web:start": "parcel ./web-client/index.html -p 1234 --out-dir web-client/dist",
    "web:build": "parcel build web-client/index.html --out-dir web-client/dist",
    "script:start": "parcel user-script/index.html -p 3000 --out-dir user-script/dist",
    "script:build": "parcel build user-script/index.html --out-dir user-script/dist",
    "parse-locales": "i18next --config web-client/i18next-parser.config.js",
    "build-ts": "tsc -b -v",
    "build-ts:watch": "tsc -b -v -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincaslt/bookings-manager.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vincaslt/bookings-manager/issues"
  },
  "homepage": "https://github.com/vincaslt/bookings-manager",
  "staticFiles": {
    "watcherGlob": "**",
    "staticPath": [
      {
        "outDirPattern": "**/user-scipt/dist",
        "staticPath": "user-script/public"
      },
      {
        "outDirPattern": "**/web-client/dist",
        "staticPath": "web-client/public"
      }
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.7.5",
    "@types/ramda": "^0.26.15",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "4.1.8",
    "@types/wouter": "^2.0.1",
    "@types/yup": "^0.26.13",
    "acorn": "^6.1.1",
    "acorn-jsx": "4.x",
    "babel-core": "^6.26.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.12.13",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@jbuschke/formik-antd": "^1.0.0-preview.6",
    "@reach/router": "^1.2.1",
    "@toast-ui/react-calendar": "^1.0.3",
    "antd": "^3.23.2",
    "axios": "^0.18.0",
    "formik": "^1.5.4",
    "i18next": "^17.0.15",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-parser": "git+https://github.com/vincaslt/i18next-parser.git#support-tag-literals",
    "i18next-xhr-backend": "^3.1.2",
    "moment": "^2.24.0",
    "path-to-regexp": "^3.0.0",
    "ramda": "^0.26.1",
    "reach-router": "^0.0.0",
    "react": "^16.8.6",
    "react-aspect-ratio": "^1.0.40",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.9.0",
    "styled-components": "^4.3.2",
    "tui-calendar": "^1.12.5",
    "wouter": "^2.2.1",
    "yup": "^0.27.0",
    "zoid": "^9.0.31"
  }
}

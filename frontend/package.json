{
  "name": "bookaquest",
  "version": "1.0.0",
  "description": "Escape room booking management system",
  "scripts": {
    "start": "npm run build-tailwind && start \"\" npm run web:start & start \"\" npm run script:start & start \"\" npm run build-ts:watch",
    "start:windows": "start /b npm run web:start & start /b npm run script:start & start /b npm run build-ts:watch",
    "start:unix": "npm run web:start & npm run script:start & npm run build-ts:watch",
    "web:i18n": "i18next --config web-client/i18next-parser.config.js",
    "web:start": "parcel ./web-client/index.html -p 1234 --out-dir web-client/dist",
    "web:build": "parcel build web-client/index.html --out-dir web-client/dist",
    "script:i18n": "i18next --config user-script/i18next-parser.config.js",
    "script:start": "parcel user-script/index.html -p 3000 --out-dir user-script/dist",
    "script:build": "parcel build user-script/index.html --out-dir user-script/dist",
    "i18n": "npm run web:i18n & npm run script:i18n",
    "build-tailwind": "tailwind build node_modules/tailwindcss/tailwind.css -o commons/tailwind.css -c commons/tailwind.config.js",
    "build-ts": "tsc -b -v",
    "build-ts:watch": "tsc -b -v -w ./commons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincasltbookaquest.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vincasltbookaquest/issues"
  },
  "homepage": "https://bookaquest.com",
  "staticFiles": {
    "watcherGlob": "**",
    "staticPath": [
      {
        "outDirPattern": "**/user-scipt/dist",
        "staticPath": "user-script/public"
      },
      {
        "outDirPattern": "**/web-client/dist",
        "staticPath": "web-client/public"
      }
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/date-fns": "^2.6.0",
    "@types/node": "^12.7.5",
    "@types/ramda": "^0.26.15",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "4.1.8",
    "@types/wouter": "^2.0.1",
    "@types/yup": "^0.26.13",
    "acorn": "^6.1.1",
    "acorn-jsx": "4.x",
    "babel-core": "^6.26.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.12.13",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@ant-design/colors": "^3.2.2",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@jbuschke/formik-antd": "^1.0.0-preview.6",
    "@reach/router": "^1.2.1",
    "@toast-ui/react-calendar": "^1.0.3",
    "antd": "^3.23.2",
    "axios": "^0.18.0",
    "formik": "^1.5.4",
    "i18next": "^17.0.15",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-parser": "git+https://github.com/vincaslt/i18next-parser.git#support-tag-literals",
    "i18next-xhr-backend": "^3.1.2",
    "moment": "^2.24.0",
    "path-to-regexp": "^3.0.0",
    "ramda": "^0.26.1",
    "reach-router": "^0.0.0",
    "react": "^16.8.6",
    "react-aspect-ratio": "^1.0.40",
    "react-countdown-now": "^2.1.1",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.9.0",
    "react-popper": "^1.3.4",
    "react-use": "^12.2.2",
    "styled-components": "^4.3.2",
    "tailwindcss": "^1.1.2",
    "timezone-support": "^2.0.2",
    "tui-calendar": "^1.12.5",
    "tzjs": "^1.0.4",
    "user-script": "^1.1.1",
    "wouter": "^2.2.1",
    "yup": "^0.27.0",
    "zoid": "^9.0.31"
  }
}
